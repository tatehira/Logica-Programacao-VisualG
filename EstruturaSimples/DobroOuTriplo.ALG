algoritmo "DobroTriplo"
var
   numero, dobro, triplo: real
inicio
      escreva("Digite um número: ")
      leia(numero)
       dobro <- numero * 2
       triplo <- numero * 3
      escreval("O dobro do número digitado é: ", dobro)
      escreval("O dobro do número digitado é: ", triplo)
fimalgoritmo